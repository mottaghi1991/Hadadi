@using Core.Enums
<div class="card card-primary" >

    <div class="card-header">آزمون </div>
    <div class="card-body">

        <div class="row">

            <div class="col-md-2">
                <div class="card card-info text-center">

                    <div class="card-header">هالند </div>
                    <div class="card-body">

                        <a class="btn btn-success" asp-action="GetUserDoneExam" asp-route-ExamId="@((int)ExamId.Haland)">شرکت کننده ها</a>
                    </div>
                </div>

            </div>


            <div class="col-md-2">
                <div class="card card-dark text-center">

                    <div class="card-header">MBTI</div>
                    <div class="card-body">

                        <a class="btn btn-success" asp-action="GetUserDoneExam" asp-route-ExamId="@((int)ExamId.MBTI)">شرکت کننده ها</a>
                    </div>
                </div>

            </div>

            <div class="col-md-2">
                <div class="card card-warning text-center">

                    <div class="card-header">اناگرام </div>
                    <div class="card-body">

                        <a class="btn btn-success" asp-action="GetUserDoneExam" asp-route-ExamId="@((int)ExamId.Anageram)">شرکت کننده ها</a>
                    </div>
                </div>

            </div>

         
        </div>
    
